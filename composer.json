{
  "name": "wergh/remote-api-login",
  "description": "A Laravel package to login against Laragel api without a login form in the device",
  "type": "library",
  "version": "0.1.0",
  "license": "MIT",
  "authors": [
    {
      "name": "Carlos LÃ³pez",
      "email": "carlos.cousillas@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Wergh\\RemoteApiLogin\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wergh\\RemoteApiLogin\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Wergh\\RemoteApiLogin\\Providers\\RemoteApiLoginServiceProvider"
      ],
      "aliases": {
        "RemoteApiLogin": "Wergh\\RemoteApiLogin\\Facades\\RemoteApiLogin"
      }
    }
  }
}
